<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >



    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingTop="30dp"
        android:paddingBottom="30dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/order_form_fragment_clicle_shape"
        >

        <TextView
            android:id="@+id/present_save_first_examine_postpone_dialog_lible"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:text="寄存有效时长：17.09.02-18.12.01\n延长期限至"
            android:textSize="13sp"
            android:textColor="#737373"
            android:gravity="center"
            />



        <LinearLayout
            android:id="@+id/present_save_first_examine_postpone_dialog_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/present_save_first_examine_postpone_dialog_lible"
            android:layout_marginTop="15dp"
            android:gravity="center_vertical"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:padding="2dp"
                android:background="@drawable/pryment_adency_fragment_government_edittext_shape"
                >

                <EditText
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_null"
                    android:padding="4dp"
                    android:textSize="13sp"
                    />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/fragment_present_save_home_image_02"
                    android:layout_marginRight="4dp"
                    />


            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="13sp"
                android:text="月"
                android:layout_marginLeft="6dp"

                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:padding="2dp"
                android:background="@drawable/pryment_adency_fragment_government_edittext_shape"
                android:layout_marginLeft="4dp"
                >

                <EditText
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_null"
                    android:padding="4dp"
                    android:textSize="13sp"
                    />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/fragment_present_save_home_image_02"
                    android:layout_marginRight="4dp"
                    />


            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="13sp"
                android:text="年"
                android:layout_marginLeft="6dp"
                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:padding="2dp"
                android:background="@drawable/pryment_adency_fragment_government_edittext_shape"
                android:layout_marginLeft="4dp"
                >

                <EditText
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_null"
                    android:padding="4dp"
                    android:textSize="13sp"
                    />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/fragment_present_save_home_image_02"
                    android:layout_marginRight="4dp"
                    />


            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="13sp"
                android:text="日"
                android:layout_marginLeft="6dp"
                />

        </LinearLayout>


        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/present_save_first_examine_postpone_dialog_layout"
            android:layout_marginTop="20dp"

            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                >

                <TextView
                    android:id="@+id/present_save_first_examine_postpone_dialog_false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="确定"
                    android:textSize="13sp"
                    android:textColor="@color/white"
                    android:background="#737373"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    />
            </LinearLayout>


        </LinearLayout>

    </RelativeLayout>
</LinearLayout>