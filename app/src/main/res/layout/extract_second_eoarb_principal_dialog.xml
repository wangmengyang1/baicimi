<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >



    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingTop="25dp"
        android:paddingBottom="25dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/order_form_fragment_clicle_shape"
        >

        <TextView
            android:id="@+id/extract_second_eoarb_principal_dialog_lible"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:text="温馨提示"
            android:textSize="13sp"
            android:textColor="#737373"
            android:gravity="center"

            />



        <TextView
            android:id="@+id/extract_second_eoarb_principal_dialog_content"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:text="提取本金后，定期存款合约解除，\n您确定要提取本金吗？"
            android:layout_below="@+id/extract_second_eoarb_principal_dialog_lible"
            android:layout_marginTop="20dp"
            android:layout_centerHorizontal="true"
            android:textSize="13sp"
            android:textColor="#737373"
            android:gravity="center"
            />


        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/extract_second_eoarb_principal_dialog_content"
            android:layout_marginTop="20dp"
            >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                >

                <TextView
                    android:id="@+id/extract_second_eoarb_principal_dialog_true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="确认"
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:background="#dcdcdc"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                >

                <TextView
                    android:id="@+id/extract_second_eoarb_principal_dialog_false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="取消"
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:background="#dcdcdc"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    />
            </LinearLayout>


        </LinearLayout>

    </RelativeLayout>
</LinearLayout>