<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D4D4D4"
   >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#DB2950">

        <ImageView
            android:id="@+id/login_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:src="@mipmap/red_1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="个人信息"
            android:textColor="#ffffff"
            android:textSize="20sp" />

    </RelativeLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:background="#ffffff"
            android:layout_height="50dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="添加个人信息，让幂信用为你全面的评估"
                android:textSize="15sp"
                android:textColor="#A7A7A7"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="基本信息"
                    android:textSize="15sp"
                    android:textColor="#A7A7A7"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/xingxing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/xingxing"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="学历学籍"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/xingxing"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="20dp"
                android:background="#D4D4D4"
                ></View>


            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/xingxing2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/xingxing"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="单位邮箱"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/xingxing2"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>



            <RelativeLayout
                android:id="@+id/zhiye_xinxi"
                android:clickable="true"
                android:onClick="onClick"
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/xingxing3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/danweixinxi"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="职业信息"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/xingxing3"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>



            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/xingxing4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/haiwaixinyong"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="海外信用报告"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/xingxing4"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="资产情况"
                    android:textSize="15sp"
                    android:textColor="#A7A7A7"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>


            <RelativeLayout
                android:id="@+id/cheliang_re"
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:clickable="true"
                android:onClick="onClick"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/cheliangxinxi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/haiwaixinyong"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车辆信息"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/cheliangxinxi"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                android:layout_marginLeft="20dp"
                ></View>


            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#ffffff"
                android:layout_height="50dp">

                <ImageView
                    android:id="@+id/gongjijin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/haiwaixinyong"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="公积金"
                    android:textSize="15sp"
                    android:textColor="#545454"
                    android:layout_toRightOf="@id/gongjijin"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_left"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#D4D4D4"
                ></View>


        </LinearLayout>

    </ScrollView>

</RelativeLayout>
