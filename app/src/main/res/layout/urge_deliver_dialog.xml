<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/commonality_transfer_dialog_layout_shape"
        android:paddingTop="25dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:paddingBottom="25dp"
        >

        <TextView
            android:id="@+id/urge_deliver_dialog_lible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="催促成功"
            android:textColor="#737373"
            android:textSize="15sp"
            android:layout_centerHorizontal="true"
            />

        <TextView
            android:id="@+id/urge_deliver_dialog_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="百次幂客服会尽快处理您的请求"
            android:textSize="14sp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/urge_deliver_dialog_lible"
            android:layout_marginTop="12dp"
            />

    </RelativeLayout>

</LinearLayout>