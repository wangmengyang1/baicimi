<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >



            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e1e1e1"
                />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="带*的为必填"
                android:textSize="13sp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="30dp"
                android:textColor="#737373"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*用户名 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入你的用户名"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*设置密码 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入6-8位密码，含字母数字和符号"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*确认密码 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请再次输入你的密码"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center_vertical"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*申请区域："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />


                <LinearLayout
                    android:id="@+id/bnn_distributor_register_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    >

                    <TextView
                        android:id="@+id/bnn_distributor_register_spinner_tv"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="请选择区域"

                        android:layout_marginRight="5dp"
                        android:textSize="12sp"
                        android:paddingLeft="8dp"
                        android:paddingTop="4dp"
                        android:paddingBottom="4dp"
                        />

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/xialacaidan_image_01"
                        android:scaleType="fitXY"
                        />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center_vertical"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*申请类别："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />


                <Spinner
                    android:id="@+id/bnn_distributor_register_spinner_02"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="22dp"
                    android:layout_marginLeft="15dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/bnn_diatributor_register_option_shape"
                    android:scrollbars="none"
                    ></Spinner>


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xialacaidan_image_01"
                    android:scaleType="fitXY"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="推荐人ID ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入推荐人ID"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text=" 推荐人姓名 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:hint="请输入推荐人姓名"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:layout_marginLeft="13dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text=" 推荐人电话 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:hint="请输入推荐人电话"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    android:layout_marginLeft="13dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*单位名称 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入单位/店铺全称"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*注册号 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入单位注册号"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*法人代表 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入法人代表姓名"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*身份证号码 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入法人身份证号"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*单位电话 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输入单位/店铺电话号码"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*详细地址 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/person_centered_acthentication_user_fragment_spinnert_shape"
                    android:textColor="#5d5d5d"
                    android:layout_marginLeft="13dp"
                    android:hint="请输单位详细地址"
                    android:textSize="12sp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="8dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="*营业执照 ："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    />
                <TextView
                    android:id="@+id/bnn_distributor_register_option_license"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 添 加 "
                    android:textSize="12sp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:background="@drawable/person_cnetered_acthentication_user_button_shape"
                    android:layout_marginLeft="13dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="11sp"
                    android:text="营业执照需盖章，过期无效"
                    android:layout_marginLeft="4dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="40dp"
                >

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="店铺门面照："
                    android:textSize="13sp"
                    android:textColor="#737373"
                    android:gravity="right"
                    android:id="@+id/textView9" />


                <TextView
                    android:id="@+id/bnn_distributor_register_option_addlayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 添 加 "
                    android:textSize="12sp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:background="@drawable/person_cnetered_acthentication_user_button_shape"
                    android:layout_marginLeft="12dp"
                    />

                <LinearLayout
                    android:id="@+id/bnn_distributor_register_option_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:visibility="invisible"
                    >
                    <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="图片"
                    android:textSize="12sp"
                    android:padding="6dp"
                    android:background="@drawable/person_cnetered_acthentication_user_button_shape"
                    android:layout_marginLeft="6dp"
                    />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="图片"
                        android:textSize="12sp"
                        android:padding="6dp"
                        android:background="@drawable/person_cnetered_acthentication_user_button_shape"
                        android:layout_marginLeft="6dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="图片"
                        android:textSize="12sp"
                        android:padding="6dp"
                        android:background="@drawable/person_cnetered_acthentication_user_button_shape"
                        android:layout_marginLeft="6dp"
                        />
                </LinearLayout>



            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="11sp"
                android:text="门面东西南北方向图片（4张）公司和大型超市可不填"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="40dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="20dp"
                >

                <CheckBox
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:background="@drawable/gouwuche_use_jifen_chexbok"
                    android:button="@null"
                    android:checked="true"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="是否已签分销/批发合约                          "
                    android:textSize="11sp"
                    android:textColor="#be000000"
                    android:layout_marginLeft="10dp"
                    />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="10dp"
                >

                <CheckBox
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:background="@drawable/gouwuche_use_jifen_chexbok"
                    android:button="@null"
                    android:checked="true"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我已阅读并接受 "
                    android:textSize="11sp"
                    android:textColor="#be000000"
                    android:layout_marginLeft="10dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="《百次幂注册协议》"
                    android:textSize="11sp"
                    android:textColor="#ce2538"
                    />

            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="立即注册"
                android:background="@drawable/activity_present_check_adapter_btn_shape_second"
                android:textColor="@color/white"
                android:gravity="center"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="8dp"
                android:textSize="16sp"
                android:paddingLeft="40dp"
                android:paddingRight="40dp"
                android:layout_gravity="center"
                />


        </LinearLayout>

    </ScrollView>


</LinearLayout>