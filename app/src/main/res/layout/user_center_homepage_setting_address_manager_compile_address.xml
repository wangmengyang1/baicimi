<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        >

        <ImageView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_address_back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/back_image_black"
            android:padding="4dp"
            android:layout_marginLeft="15dp"
            />

        <TextView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_address_lible"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="修改收货地址"
            android:textColor="#535353"
            android:gravity="center"
            android:textSize="16sp"
            />

        <TextView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_saveaddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textColor="#ff0000"
            android:text="保存"
            android:paddingRight="15dp"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.4dp"
        android:background="#e1e1e1"
        />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:src="@drawable/setting_user_message_basic_date_image_01"
        android:scaleType="fitXY"
        />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >


        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/username_image"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="8dp"
            android:text="收货人"
            android:textColor="#60646F"
            android:textSize="13sp" />

        <EditText
            android:id="@+id/ed_receive_man"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_null"
            android:layout_marginLeft="5dp"
            android:textColor="#5B5B5B"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >


        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/phone_image_01"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="8dp"
            android:text="联系电话"
            android:textColor="#60646F"
            android:textSize="13sp" />


        <EditText
            android:id="@+id/user_center_homepage_setting_address_manager_compile_address_num"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_null"
            android:layout_marginLeft="5dp"
            android:textColor="#5B5B5B"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            />


    </LinearLayout>


    <RelativeLayout
        android:id="@+id/user_center_homepage_setting_address_manager_compile_address_area"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >

        <ImageView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_address_address"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/address_image"

            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="8dp"
            android:text="所在地区"
            android:textColor="#60646F"
            android:textSize="13sp"
            android:layout_toRightOf="@+id/user_center_homepage_setting_address_manager_compile_address_address"
            />


        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@mipmap/right_img"></ImageView>

        <TextView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_addresslible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="35dp"
            android:text="请选择"
            android:textColor="#AAAAAA"
            android:textSize="13sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >

        <ImageView
            android:id="@+id/user_center_homepage_setting_address_manager_compile_address_addressjd"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/address_image"
            android:visibility="invisible"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="8dp"
            android:text="街道"
            android:textColor="#60646F"
            android:textSize="13sp"
            android:layout_toRightOf="@+id/user_center_homepage_setting_address_manager_compile_address_addressjd"

            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@mipmap/right_img"></ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="35dp"
            android:text="请选择"
            android:textColor="#AAAAAA"
            android:textSize="13sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="1dp"
        android:background="#FFFFFF">


        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_null"
            android:layout_marginLeft="10dp"
            android:textColor="#5B5B5B"
            android:gravity="top"
            android:layout_marginRight="10dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            android:paddingLeft="15dp"
            android:paddingBottom="15dp"
            android:hint="民营科技园火炬大厦1501"
            android:textSize="13sp"
            android:paddingTop="15dp"
            />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >


        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/phone_image_01"
            android:visibility="invisible"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="8dp"
            android:text="邮箱"
            android:textColor="#60646F"
            android:textSize="13sp" />


        <EditText

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_null"
            android:layout_marginLeft="5dp"
            android:textColor="#5B5B5B"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            />


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        >


        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/phone_image_01"
            android:visibility="invisible"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="8dp"
            android:text="邮编"
            android:textColor="#60646F"
            android:textSize="13sp"

            />


        <EditText

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/background_null"
            android:layout_marginLeft="5dp"
            android:textColor="#5B5B5B"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="10dp"
            />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#eeeeee"
        />

    <RelativeLayout
        android:id="@+id/user_center_homepage_setting_address_manager_compile_address_saveaddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="删除地址"
            android:textSize="13sp"
            android:textColor="#ff0000"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            />


    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#eeeeee"
        />


</LinearLayout>