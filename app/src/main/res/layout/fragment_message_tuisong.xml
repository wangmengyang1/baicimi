<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/login_title"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <ImageView
                android:id="@+id/login_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="25dp"
                android:src="@mipmap/register_back" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="消息推送"
                android:textColor="#000000"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/chuangkou"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@mipmap/cehua" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#EEEEEE"></View>

        <ImageView
            android:id="@+id/login_guanggao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/login_title"
            android:layout_marginTop="-30dp"
            android:src="@mipmap/login_title" />

        <View
            android:id="@+id/vv1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="-30dp"
            android:background="#EEEEEE"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#F5F4F9"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="#FFFFFF"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="消息提醒"
                    android:textSize="13sp"
                    android:textColor="#A1A0A3"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E1E0E5"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#FFFFFF"
                android:layout_height="60dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    >

                    <TextView
                        android:id="@+id/ringing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="新消息提醒"
                        android:textSize="16sp"
                        android:textColor="#59595B"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="8dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="开启后可接收新消息提醒"
                        android:textSize="13sp"
                        android:layout_below="@id/ringing"
                        android:textColor="#B0B0B2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        />
                </RelativeLayout>

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:button="@null"
                    android:background="@drawable/yesnot_phone_find"
                    />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E1E0E5"
                ></View>

            <View
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="#F5F4F9"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="#FFFFFF"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提示音效"
                    android:textSize="13sp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textColor="#9D9DA0"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E1E0E5"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#FFFFFF"
                android:layout_height="60dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    >

                    <TextView
                        android:id="@+id/zhendong"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="铃声"
                        android:textSize="16sp"
                        android:textColor="#59595B"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="8dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="通知时铃声提醒"
                        android:textSize="13sp"
                        android:layout_below="@id/zhendong"
                        android:textColor="#B0B0B2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        />
                </RelativeLayout>

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:button="@null"
                    android:background="@drawable/yesnot_phone_find"
                    />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E1E0E5"
                ></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:background="#FFFFFF"
                android:layout_height="60dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    >

                    <TextView
                        android:id="@+id/new_message"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="振动"
                        android:textSize="16sp"
                        android:textColor="#59595B"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="8dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="通知时振动提醒"
                        android:textSize="13sp"
                        android:layout_below="@id/new_message"
                        android:textColor="#B0B0B2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"
                        />
                </RelativeLayout>

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:button="@null"
                    android:background="@drawable/yesnot_phone_find"
                    />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E1E0E5"
                ></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#ffffff"
                ></LinearLayout>

        </LinearLayout>

    </LinearLayout>


</RelativeLayout>
