<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingTop="4dp"
        >

        <ImageView
            android:id="@+id/mi_integral_brank_my_tf_cd_first_second_back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/back_image_black"
            android:padding="4dp"
            android:layout_marginLeft="15dp"
            />

        <TextView
            android:id="@+id/mi_integral_brank_my_tf_cd_first_second_lible"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="活期储蓄"
            android:textColor="#737373"
            android:gravity="center"
            android:textSize="16sp"
            />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/brand_fragment_second_image_13"
            android:padding="6dp"
            android:layout_marginRight="15dp"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="18dp"
        >

        <ImageView
            android:id="@+id/mi_integral_brank_my_tf_cd_first_second_scanning"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/brand_fragment_second_image_12"
            android:layout_marginLeft="15dp"
            />

        <EditText
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="28dp"
            android:background="@drawable/brand_fragment_second_edittext_layer_list"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:hint=""
            android:textSize="12sp"
            android:paddingLeft="8dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="搜索"
            android:textSize="13sp"
            android:layout_marginRight="15dp"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.4dp"
        android:background="#e1e1e1"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:orientation="horizontal"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:background="@drawable/pryment_adency_fragment_government_edittext_shape"
        >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="13sp"
            android:text="存款形式：整存零取\n存款金额：5000"
            />

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="13sp"
            android:text="存入时间：2017.02.20"
            />



    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13sp"
        android:text="存取记录"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="vertical"
        android:background="@drawable/pryment_adency_fragment_government_edittext_shape"
        >




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >



            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="时间"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"
                    android:background="#dcdcdc"
                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    android:layout_marginTop="0.6dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2017.04.10"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />

                <View
                    android:layout_width="match_parent"

                    android:layout_height="0.6dp"
                    android:background="#737373"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2017.04.10"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />



            </LinearLayout>
            <View
                android:layout_width="0.6dp"
                android:layout_height="85dp"
                android:background="#737373"
                />
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="动作"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"
                    android:background="#dcdcdc"
                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    android:layout_marginTop="0.6dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="取出"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />

                <View
                    android:layout_width="match_parent"

                    android:layout_height="0.6dp"
                    android:background="#737373"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="存入"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />



            </LinearLayout>
            <View
                android:layout_width="0.6dp"
                android:layout_height="85dp"
                android:background="#737373"
                />
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="金额"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"
                    android:background="#dcdcdc"
                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    android:layout_marginTop="0.6dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="400"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />

                <View
                    android:layout_width="match_parent"

                    android:layout_height="0.6dp"
                    android:background="#737373"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="400"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />



            </LinearLayout>
            <View
                android:layout_width="0.6dp"
                android:layout_height="85dp"
                android:background="#737373"
                />
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="余额"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"
                    android:background="#dcdcdc"
                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    android:layout_marginTop="0.6dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="400"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />

                <View
                    android:layout_width="match_parent"

                    android:layout_height="0.6dp"
                    android:background="#737373"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="400"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"

                    android:textColor="#737373"
                    android:layout_marginLeft="0.6dp"
                    />



            </LinearLayout>

        </LinearLayout>

    </LinearLayout>



</LinearLayout>