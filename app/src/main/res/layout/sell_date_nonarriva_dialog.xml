<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        android:background="@drawable/order_form_fragment_clicle_shape"
        >

        <TextView
            android:id="@+id/sell_date_nonarriva_dialog_lible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="对不起，还未到发售时间。"
            android:textSize="13sp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            />

        <View
            android:id="@+id/sell_date_nonarriva_dialog_view"
            android:layout_width="180dp"
            android:layout_height="0.6dp"
            android:background="#737373"
            android:layout_below="@+id/sell_date_nonarriva_dialog_lible"
            android:layout_marginTop="30dp"
            />

        <TextView
            android:id="@+id/sell_date_nonarriva_dialog_dissmess"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="确定"
            android:layout_below="@+id/sell_date_nonarriva_dialog_view"
            android:layout_centerHorizontal="true"

            android:padding="15dp"
            />


    </RelativeLayout>

</LinearLayout>